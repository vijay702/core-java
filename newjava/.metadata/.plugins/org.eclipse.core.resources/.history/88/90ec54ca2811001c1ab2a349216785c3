package org.studyeasy;

public class Person {

	public String name ="john";
	public int age = 25;
	public String gender ;
	

	public String getName() {
		return name;
	}



	public int getAge() {
		return age;
	}


	public String getGender() {
		return gender;
	}


	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + ", gender=" + gender + "]";
	}
	
	
	
}
